<?xml version="1.0" encoding="UTF-8" ?>
<ResourceDictionary 
    xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
    xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml">

    <Color x:Key="Primary">#79996D</Color>

    <Color x:Key="WarningRed">#A43935</Color>
    <Color x:Key="WarningOrange">#EEC054</Color>


    <Color x:Key="White">#FFFFFF</Color>
    <Color x:Key="Black">#000000</Color>

    <Color x:Key="LightAppBackground">#F7F2E2</Color>
    <Color x:Key="LightCardBackground">#FFFFFF</Color>
    <Color x:Key="LightText">#333333</Color>
    <Color x:Key="LightBorder">#E0E0E0</Color>

    <Color x:Key="DarkAppBackground">#121212</Color>
    <Color x:Key="DarkCardBackground">#393939</Color>
    <Color x:Key="DarkText">#d8d8d8</Color>
    <Color x:Key="DarkBorder">#79996D</Color>
</ResourceDictionary>